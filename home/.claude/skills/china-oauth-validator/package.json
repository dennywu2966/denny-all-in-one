{
  "name": "china-oauth-validator",
  "version": "1.0.0",
  "description": "General-purpose OAuth validation for Chinese services (Aliyun, WeChat) with automated SMS handling",
  "type": "module",
  "main": "validate_china_oauth.js",
  "bin": {
    "validate_china_oauth": "./validate_china_oauth.js"
  },
  "scripts": {
    "validate": "node validate_china_oauth.js",
    "validate:aliyun": "node validate_china_oauth.js --provider aliyun",
    "validate:wechat": "node validate_china_oauth.js --provider wechat",
    "help": "node validate_china_oauth.js --help"
  },
  "keywords": [
    "oauth",
    "china",
    "aliyun",
    "wechat",
    "sms",
    "validation",
    "automation",
    "playwright"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.49.0",
    "commander": "^12.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
