{
    "$schema": "https://opencode.ai/config.json",
    "plugin": ["opencode-antigravity-auth@latest"],
    "mcp": {
        "filesystem": {
            "type": "local",
            "command": ["bun", "x", "@modelcontextprotocol/server-filesystem", "$REPO"]
        },
        "ripgrep": {
            "type": "local",
            "command": ["bun", "x", "mcp-ripgrep@latest"]
        },
        "ast_grep": {
            "type": "local",
            "command": ["uvx", "--from", "ast-grep-mcp", "ast-grep-mcp"]
        },
        "mermaid": {
            "type": "local",
            "command": ["bunx", "-y", "claude-mermaid"],
            "enabled": true
        },
        "zai-mcp-server": {
            "type": "local",
            "command": ["npx","-y","@z_ai/mcp-server"]
        },
        "web-reader": {
            "type": "remote",
            "url": "https://open.bigmodel.cn/api/mcp/web_reader/mcp",
            "headers": {
                "Authorization": "Bearer 74830934db8146fb84b2c12daa182d5f.NnK1nfrYHm4Tqdgc"
            }
        },
        "web-search-prime": {
            "type": "remote",
            "url": "https://open.bigmodel.cn/api/mcp/web_search_prime/mcp",
            "headers": {
                "Authorization": "Bearer 74830934db8146fb84b2c12daa182d5f.NnK1nfrYHm4Tqdgc"
            }
        },
        "zread": {
            "type": "remote",
            "url": "https://open.bigmodel.cn/api/mcp/zread/mcp",
            "headers": {
                "Authorization": "Bearer 74830934db8146fb84b2c12daa182d5f.NnK1nfrYHm4Tqdgc"
            }
        },
        "jina": {
            "type": "remote",
            "url": "https://mcp.jina.ai/v1",
            "headers": {
                "Authorization": "Bearer jina_4d22586fca5140e99831e91c67f7b09aBX3XfmHSkXlBEhn3PvJna9cZYOXb"
            }
        },
        "rube": {
            "type": "remote",
            "url": "https://rube.app/mcp"
        },
        "playwright": {
            "type": "local",
            "command": [
                "bunx",
                "@playwright/mcp@latest",
                "--headless"
            ]
        }
    },
